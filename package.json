{
  "name": "ai-copy-guard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:suite",
    "start:suite": "concurrently \"npm run start:server\" \"npm run start:dashboard\" \"npm run start:browser\" \"npm run start:ide\"",
    "start:server": "cd server && npm start",
    "start:dashboard": "ng serve security-dashboard --port 4200",
    "start:browser": "ng serve browser-guard --port 4201",
    "start:ide": "ng serve ide-guard --port 4202",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.2.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@google/genai": "^1.31.0",
    "@google/generative-ai": "^0.24.1",
    "lucide-angular": "^0.555.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/canvas-confetti": "^1.9.0",
    "autoprefixer": "^10.4.22",
    "canvas-confetti": "^1.9.4",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
